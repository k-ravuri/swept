    "def DFS(graph, temp, v, visited): \n",
    "\n",
    "    # Mark the current vertex as visited \n",
    "    visited[v] = True\n",
    "\n",
    "    # Store the vertex to list \n",
    "    temp.append(v) \n",
    "\n",
    "    # Repeat for all vertices adjacent \n",
    "    # to this vertex v \n",
    "    for i in graph.neighbors(v): \n",
    "        if visited[i] == False: \n",
    "\n",
    "            # Update the list \n",
    "            temp = DFSUtil(graph, temp, i, visited) \n",
    "    return temp\n",
    "\n",
    "def connectedComponents(graph): \n",
    "    visited = {}\n",
    "    cc = [] \n",
    "    vertices = list(graph.vertices())\n",
    "    for i in (vertices): \n",
    "        visited[i] = False\n",
    "    for v in (vertices): \n",
    "        if visited[v] == False: \n",
    "            temp = [] \n",
    "            cc.append(DFSUtil(graph, temp, v, visited)) \n",
    "    return cc \n",
    "connectedComponents(graph)\n",
    "\n",
    "def inter_component_plan(graph, start):\n",
    "    path = []\n",
    "    curr = start\n",
    "    unvisited = set(list(graph.vertices()))\n",
    "    visited = set()\n",
    "    while len(visited) < len(graph.vertices()):\n",
    "        path.append(curr)\n",
    "        visited.add(curr)\n",
    "        unvisited.remove(curr)\n",
    "        past = curr\n",
    "        curr = None\n",
    "        for n in graph.neighbors(past):\n",
    "            if n not in visited:\n",
    "                curr = n\n",
    "                break\n",
    "        if curr == None:\n",
    "            if len(unvisited) > 0:\n",
    "                curr = list(unvisited)[0]\n",
    "            else:\n",
    "                break\n",
